<spider name="meituan">
<!-- 	<urlset urltemplate="http://item.jd.com/{1}.html" temp1type="int" temp1start="1" temp1end="1000000"></urlset> -->

	<!-- 城市 -->
	<url name="city" href="http://www.meituan.com/index/changecity/initiative"></url>
	<datatran name="cityDataProcess" input="city" select=".hasallcity li a.isonline" attr="href" regex="http://([\d\w]+)"></datatran>
	
	<!-- 分类 -->
	<url name="category" href="http://cd.meituan.com"></url>
	<datatran name="categoryDataProcess" input="category" select=".nav-level2-keywords--content li a.keywords__item" attr="href" regex="/([\d\w]+)$"></datatran>
	
	<!-- 列表 -->
	<urlset urltemplate="http://{1}.meituan.com/category/{2}" temp1type="enum" temp2type="enum">
		<temp1value>cd;bj</temp1value>
		<temp2value>meishi</temp2value>
		<paging select=".paginator-wrapper .paginator a" urlattr="href"></paging>
	</urlset>
</spider>